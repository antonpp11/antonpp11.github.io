---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>antonpp11's projects</title>
	<link href="http://antonpp11.github.io/projects/atom.xml" rel="self"/>
	<link href="http://antonpp11.github.io/projects"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://antonpp11.github.io/projects</id>
	<author>
		<name>antonpp11</name>
		<email>antonpellerinpetrov@gmail.com</email>
	</author>

	{% for project in site.projects %}
		<entry>
			<title>{{ project.title }}</title>
			<link href="http://antonpp11.github.io{{ project.url }}"/>
			<updated>{{ project.date | date_to_xmlschema }}</updated>
			<id>http://antonpp11.github.io{{ project.id }}</id>
			<content type="html">{{ project.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>